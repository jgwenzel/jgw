<?xml version="1.0"?>
<!-- DoneLogic/Core/etc/acl.xml -->
<!-- @author John Wenzel johngwenzel@gmail.com -->
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="DoneLogic_Core::core" title="DoneLogic Core" sortOrder="51">
                    <resource id="DoneLogic_Core::record" title="Records" sortOrder="10"/>
                    <resource id="DoneLogic_Core::core_configuration" title="DoneLogic Core Configuration" sortOrder="99" />
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="DoneLogic_Core::core_config" title="DoneLogic Core Config"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>